{
  "sql_servers": [
    {
      "host": "db",
      "port": 5432,
      "tls_config": {
        "enabled": true
      },
      "databases": {
        "notes": {
          "max_connections": 100,
          "min_connections": 1,
          "username": {
            "$env": "POSTGRES_USER"
          },
          "password": {
            "$env": "POSTGRES_PASSWORD"
          }
        }
      }
    }
  ]
}
