{
    "sql_servers": [
        {
            "host": "cnpg-webhook-service.cnpg-db-stg.svc.cluster.local",
            "port": 443,
            "tls_config": {
                "disabled": true
            },
            "databases": {
                "notes": {
                    "max_connections": 100,
                    "min_connections": 1,
                    "username": {
                        "$env": "POSTGRES_USER"
                    },
                    "password": {
                        "$env": "POSTGRES_PASSWORD"
                    }
                }
            }
        }
    ]
}
